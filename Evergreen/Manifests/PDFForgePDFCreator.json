{
	"Name": "pdfforge PDFCreator",
	"Source": "https://www.pdfforge.org/pdfcreator",
	"Get": {
		"Update": {
			"Uri": "https://update.pdfforge.org/pdfcreator/updates",
			"ContentType": "application/json; charset=utf-8"
		},
		"Download" : {
			"Folder": "PDFCreator",
			"Feed": "https://sourceforge.net/projects/pdfcreator/rss?path=",
			"FilterProperty": "link",
            "ReplaceText": {
                "Link": "/download"
            },
            "MatchFileTypes": "\\.exe$|\\.msi$",
            "FileProperty": "description.'#cdata-section'",
			"ContentType": "application/rss+xml; charset=utf-8",
			"Uri": "https://nchc.dl.sourceforge.net/project/pdfcreator"
		},
		"MatchVersion": "(\\d+(\\.\\d+){1,3})",
		"DatePattern": "yyyy-MM-dd HH:mm:ss"
	},
	"Install": {
		"Setup": "WinMerge*.exe",
		"Physical": {
			"Arguments": "/Silent",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/Silent",
			"PostInstall": []
		}
	}
}
